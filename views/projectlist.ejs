<script type="text/javascript">
    $.get("/projects/listOfProjects", function (data) { // Fires an Ajax POST request

        var pn = '';
        var pd = '';
        var ci = '';

        $.each(data.detailsOfProjects, function (index, e) {
            pn+= e.pro_name+'<br><br>';
            pd+= e.pro_direc+'<br><br>';
            ci+= e.comp_name+'<br><br>';
            console.log(e);
        });

        $("#pn").append(pn);
        $("#pd").append(pd);
        $("#ci").append(ci);

    });
</script>