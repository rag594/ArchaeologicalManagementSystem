</nav>
</body>
<div class="container">
    <table class="table mar-top">
        <thead>
        <tr>
            <th>description</th>
            <th>latitude</th>
            <th>longitude</th>
            <th>type</th>
            <th>recorder name</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td id="dp"></td>
            <td id="lt"></td>
            <td id="ln"></td>
            <td id="tp"></td>
            <td id="rn"></td>
        </tr>
        </tbody>
    </table>
    <div class="form-group">
        <input type="date" placeholder="Date"  class="form-control" name="date" onchange="changeFunc();"><br>
    </div>
</div>
</html>

<script type="text/javascript">

    function changeFunc() {
        var date = $('input[name=date]').val();
        $.post("/artifacts/getArtifactByDate", {date: date}, function (data) { // Fires an Ajax POST request
            var dp = '';
            var lt = '';
            var ln = '';
            var tp = '';
            var rn = '';

            $.each(data.artifactByDate, function (index, e) {
                dp += e.description + '<br><br>';
                lt += e.latitude + '<br><br>';
                ln += e.longitude + '<br><br>';
                tp += e.object_type + '<br><br>';
                rn += e.recorde_name + '<br><br>';
            });

            $("#dp").append(dp);
            $("#lt").append(lt);
            $("#ln").append(ln);
            $("#tp").append(tp);
            $("#rn").append(rn);

        });

    }
</script>