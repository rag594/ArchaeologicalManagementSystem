<script type="text/javascript">
    $.get("/artifacts", function (data) { // Fires an Ajax POST request
    console.log(data);
                var dt = '';
                var dp = '';
                var lt = '';
                var ln = '';
                var li = '';
                var tp = '';
                var rn = '';

        $.each(data.detailsOfArtifact, function (index, e) {
             dt+= e.date.slice(0,10)+'<br><br>';
            dp+= e.description+'<br><br>';
            lt+= e.latitude+'<br><br>';
            ln+= e.longitude+'<br><br>';
            li+= e.lot_id+'<br><br>';
            tp+= e.object_type+'<br><br>';
            rn+= e.recorde_name+'<br><br>';
            console.log(e);
        });

        $("#dt").append(dt);
        $("#dp").append(dp);
        $("#lt").append(lt);
        $("#ln").append(ln);
        $("#li").append(li);
        $("#tp").append(tp);
        $("#rn").append(rn);

    });
</script>